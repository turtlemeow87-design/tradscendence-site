    {
      "name": "TC001 - Validate Successful Chat Completion Response",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status is 200\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "pm.test(\"Response has choices array\", function () {\r",
              "    const json = pm.response.json();\r",
              "    pm.expect(json).to.have.property(\"choices\");\r",
              "    pm.expect(json.choices).to.be.an(\"array\");\r",
              "    pm.expect(json.choices.length).to.be.greaterThan(0);\r",
              "});\r",
              "\r",
              "pm.test(\"Content field is not null\", function () {\r",
              "    const json = pm.response.json();\r",
              "    const content = json.choices[0].message.content;\r",
              "    pm.expect(content).to.not.be.null;\r",
              "    pm.expect(content).to.be.a(\"string\");\r",
              "    pm.expect(content.length).to.be.greaterThan(0);\r",
              "});\r",
              "\r",
              "pm.test(\"Model matches expected\", function () {\r",
              "    const json = pm.response.json();\r",
              "    pm.expect(json.model).to.include(\"kimi\");\r",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }

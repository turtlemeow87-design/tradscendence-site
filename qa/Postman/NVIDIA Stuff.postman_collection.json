    {
      "name": "TC002 - Verify the API respects max_tokens and returns usage data.",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status is 200\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "pm.test(\"Usage object is present\", function () {\r",
              "    const json = pm.response.json();\r",
              "    pm.expect(json).to.have.property(\"usage\");\r",
              "    pm.expect(json.usage).to.have.property(\"prompt_tokens\");\r",
              "    pm.expect(json.usage).to.have.property(\"completion_tokens\");\r",
              "    pm.expect(json.usage).to.have.property(\"total_tokens\");\r",
              "});\r",
              "\r",
              "pm.test(\"Completion tokens within max_tokens limit\", function () {\r",
              "    const json = pm.response.json();\r",
              "    pm.expect(json.usage.completion_tokens).to.be.at.most(50);\r",
              "});\r",
              "\r",
              "pm.test(\"Finish reason is present\", function () {\r",
              "    const json = pm.response.json();\r",
              "    const finishReason = json.choices[0].finish_reason;\r",
              "    pm.expect(finishReason).to.be.a(\"string\");\r",
              "    pm.expect([\"stop\", \"length\"]).to.include(finishReason);\r",
              "});"
